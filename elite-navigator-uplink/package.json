{
  "name": "elite-navigator-uplink",
  "version": "1.0.0",
  "description": "Elite Navigator local uplink",
  "keywords": [
    "Elite Dangerous",
    "Navigator",
    "Client"
  ],
  "author": "Ryan Vandersmith <https://rvanasa.github.io>",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "start": "set OVERLAY_URL=http://localhost:3000?layout=overlay&& electron .",
    "build": "electron-builder"
  },
  "dependencies": {
    "chokidar": "^3.4.0",
    "debounce": "^1.2.0",
    "socket.io-client": "^2.3.0",
    "string-hash": "^1.1.3",
    "tail": "^2.0.4"
  },
  "devDependencies": {
    "electron-builder": "^22.7.0",
    "electron": "^9.1.1"
  },
  "build": {
    "appId": "rvanasa.EliteNavigator",
    "artifactName": "${productName}.${ext}",
    "productName": "Elite Navigator",
    "files": [
      "**/*"
    ],
    "directories": {
      "output": "../elite-navigator-web/public/data/elite-navigator"
    },
    "win": {
      "target": "portable"
    }
  }
}
